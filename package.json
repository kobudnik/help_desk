{
  "name": "help_desk_exercise",
  "version": "1.0.0",
  "description": "Help desk / support system ticket management system.",
  "author": "Ken Budnik",
  "scripts": {
    "dev:client": "cd client && vite",
    "dev:server": "NODE_ENV=development cd server && tsc -w & tsx watch server/index.ts ",
    "dev": "npm run dev:client & npm run dev:server",
    "build:client": "cd client && vite build --emptyOutDir",
    "build:server": "cd server && tsc",
    "compose": "cd server && cd models && docker compose up -d",
    "compose:down": "cd server && cd models && docker compose down",
    "build": "npm run build:client & npm run build:server",
    "lint": "eslint --max-warnings=0",
    "start": "NODE_ENV=production node ./dist/server/index.js"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@types/pg": "^8.10.7",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/react-modal": "^3.16.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "daisyui": "^3.9.4",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.31",
    "postcss-nesting": "^12.0.1",
    "prettier": "^2.8.8",
    "react-daisyui": "^4.1.2",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
